{
  "name": "deviantart-filter",
  "title": "DeviantArt Filter",
  "version": "5.0.3",
  "description": "Allows configurable filtering/removal of deviations by user, tag, and/or category on DeviantArt",
  "author": "Ryan Thaut <rthaut@gmail.com> (http://ryanthaut.com)",
  "homepage": "https://github.com/rthaut/deviantART-Filter",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "build:development": "cross-env NODE_ENV=development env-path gulp build:development",
    "build:production": "cross-env NODE_ENV=production env-path gulp build:production",
    "clean": "trash ./dist",
    "watch:development": "cross-env NODE_ENV=development env-path gulp watch:development",
    "watch:production": "cross-env NODE_ENV=production env-path gulp watch:production",
    "firefox:lint": "web-ext lint --source-dir=./dist/firefox",
    "firefox:build": "web-ext build --source-dir=./dist/firefox --firefox=firefox",
    "firefox:start": "web-ext run --source-dir=./dist/firefox",
    "package": "cross-env NODE_ENV=production env-path gulp package",
    "test": "run-s clean build:production firefox:lint",
    "docs": "verb default"
  },
  "repository": {
    "type": "git",
    "url": "git://https://github.com/rthaut/deviantART-Filter.git"
  },
  "bugs": {
    "url": "https://github.com/rthaut/deviantART-Filter/issues"
  },
  "dependencies": {
    "angular": "^1.7.5",
    "angular-messages": "^1.7.5",
    "angular-sanitize": "^1.7.5",
    "bootstrap": "~3.4.0",
    "jquery": "^3.2.1",
    "ng-table": "^3.0.1",
    "webextension-polyfill": "^0.4.0"
  },
  "devDependencies": {
    "ansi-colors": "^3.2.3",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.8",
    "del": "^3.0.0",
    "env-path": "^1.0.6",
    "fancy-log": "^1.3.3",
    "filterhtml": "^0.6.0",
    "gulp": "^4.0.0",
    "gulp-angular-embed-templates": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-ejs": "^3.1.3",
    "gulp-eslint": "^5.0.0",
    "gulp-header": "^2.0.5",
    "gulp-if": "^2.0.2",
    "gulp-less": "^4.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-merge-json": "^1.3.1",
    "gulp-postcss": "^8.0.0",
    "gulp-print": "^5.0.0",
    "gulp-rename": "^1.2.3",
    "gulp-replace": "^1.0.0",
    "gulp-svg2png": "^2.0.2",
    "gulp-token-replace": "^1.1.2",
    "gulp-uglify": "^3.0.0",
    "gulp-useref": "^3.1.3",
    "gulp-zip": "^4.0.0",
    "html-minifier": "^3.5.16",
    "html-to-text": "^4.0.0",
    "idb": "^3.0.2",
    "ini": "^1.3.4",
    "marked": "^0.6.0",
    "merge-stream": "^1.0.1",
    "npm-run-all": "^4.1.5",
    "pump": "^3.0.0",
    "rollup-stream": "^1.24.1",
    "through2": "^3.0.0",
    "time-stamp": "^2.0.0",
    "trash-cli": "^1.4.0",
    "uglify-es": "^3.3.9",
    "verb": "github:verbose/verb#dev",
    "verb-generate-readme": "^0.8.0",
    "verb-toc": "^0.2.9",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^2.0.0",
    "web-ext": "^2.7.0"
  },
  "verb": {
    "silent": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 2
    }
  }
}
